<div class="pagination">
	<% if(currentPage > 1) { %>
	<a href="/uData/?page=1" class="first"> << </a>
	<a href="/uData/?page=<%= currentPage-1 %>" class="prev">&nbsp; < &nbsp;</a>
	<% } %>
	<p id="page"><%= currentPage %></p>
	<% if(currentPage*20 < length) { %>
	<a href="/uData/?page=<%= ++currentPage %>" class="next">&nbsp; > &nbsp;</a>
	<a href="/uData/?page=<%=length/20%>" class="last">>></a>

	<% } %>
</div>
