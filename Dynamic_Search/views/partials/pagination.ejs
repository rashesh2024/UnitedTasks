<div class="pagination">
	<% if(currentPage > 1) { %>
	<a href="<%= url %>/?page=1" class="first"> << </a>
	<a href="<%= url %>/?page=<%= currentPage-1 %>" class="prev"
		>&nbsp; < &nbsp;</a
	>
	<% } %>
	<p id="page"><%= currentPage %></p>
	<% if(currentPage*20 < length) { %>
	<a href="<%= url %>/?page=<%= ++currentPage %>" class="next"
		>&nbsp; > &nbsp;</a
	>
	<a href="<%= url %>/?page=<%=length/20%>" class="last">>></a>

	<% } %>
</div>
