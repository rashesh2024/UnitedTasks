							type="text"
							name="fname"
							id="fname"
							tabindex="1"
							placeholder="Enter Your Name"
							value="<%= data.Basic.fname ? data.Basic.fname : ''  %>"
							onkeypress="remove(this)"
						/><span class="requireErr"></span><br />
					</div>

					<div class="col">
						<label for="lname" class="form-lable col">Last Name</label>
